\\u0430\\u043C\\u043C\\u044B. \\u0412\\u0442\\u043E\\u0440\\u043E\\u0439 \\u043F\\u043B\\u0430\\u043D\\u0438\\u0440\\u0443\\u0435\\u043C\\u044B\\u0439 \\u0432\\u0430\\u0440\\u0438\\u0430\\u043D\\u0442 \\u043D\\u0430\\u0437\\u0432\\u0430\\u043D\\u0438\\u044F \\u0434\\u043E\\u043C\\u0435\\u043D\\u0430 \\u2013 univer-checklist.ru (\\u0445\\u043E\\u0442\\u044F \\u0434\\u0430\\u043D\\u043D\\u043E\\u0435 \\u043D\\u0430\\u0437\\u0432\\u0430\\u043D\\u0438\\u0435 \\u043C\\u0435\\u043D\\u0435\\u0435 \\u043B\\u0430\\u043A\\u043E\\u043D\\u0438\\u0447\\u043D\\u043E, \\u0441\\u043A\\u043E\\u0440\\u0435\\u0435 \\u0432\\u0441\\u0435\\u0433\\u043E, \\u0435\\u0433\\u043E \\u0431\\u0443\\u0434\\u0435\\u0442 \\u043B\\u0435\\u0433\\u0447\\u0435 \\u0437\\u0430\\u043F\\u043E\\u043C\\u043D\\u0438\\u0442\\u044C \\u0441\\u0442\\u0443\\u0434\\u0435\\u043D\\u0442\\u0430\\u043C).\"));\n};\n\nexport default Direction;","map":{"version":3,"sources":["/Users/r.v.mosolov/Desktop/webstorm-projects/projects/big-projects/university-checklist.ru/src/pages/about-service/about-service-full/site-content/blocks/direction/direction.js"],"names":["React","FirstPicture","Direction"],"mappings":";AAAA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAO,uBAAP;AACA,OAAOC,YAAP,MAAyB,qBAAzB;AACA,OAAO,qBAAP;;AAEA,MAAMC,SAAS,GAAG,MAAM;AACpB,SACI;AAAK,IAAA,SAAS,EAAC,WAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACI;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,2SADJ,EAEI;AAAG,IAAA,SAAS,EAAC,wBAAb;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,sjDAFJ,EAOI;AAAG,IAAA,SAAS,EAAC,wBAAb;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,mwBAPJ,EAWI;AAAG,IAAA,SAAS,EAAC,wBAAb;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,utBAXJ,EAeI;AAAG,IAAA,SAAS,EAAC,wBAAb;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,+qDAfJ,EAqBI;AAAG,IAAA,SAAS,EAAC,wBAAb;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,y2CArBJ,EA0BI;AAAG,IAAA,SAAS,EAAC,wBAAb;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,6lBA1BJ,EA8BI;AAAG,IAAA,SAAS,EAAC,wBAAb;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,yYA9BJ,EAiCI;AAAG,IAAA,SAAS,EAAC,wBAAb;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,gqCAjCJ,EAsCI;AAAG,IAAA,SAAS,EAAC,wBAAb;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,0oBAtCJ,EA0CI;AAAG,IAAA,SAAS,EAAC,wBAAb;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,yOA1CJ,EA2CI;AAAG,IAAA,SAAS,EAAC,wBAAb;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,wFA3CJ,EA4CI;AAAG,IAAA,SAAS,EAAC,wBAAb;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,mSA5CJ,EA6CI;AAAG,IAAA,SAAS,EAAC,wBAAb;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,wIA7CJ,EA8CI;AAAG,IAAA,SAAS,EAAC,wBAAb;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,qPA9CJ,EA+CI;AAAG,IAAA,SAAS,EAAC,wBAAb;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,6LA/CJ,EAgDI;AAAG,IAAA,SAAS,EAAC,mBAAb;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,2jEAhDJ,EAsDI;AAAG,IAAA,SAAS,EAAC,gDAAb;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,oDAtDJ,EAsD+E;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,IAtD/E,EAuDI;AAAG,IAAA,SAAS,EAAC,gDAAb;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,+6BAvDJ,EA2DI;AAAK,IAAA,GAAG,EAAED,YAAV;AAAwB,IAAA,GAAG,EAAC,8CAA5B;AAAwC,IAAA,SAAS,EAAC,sBAAlD;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,IA3DJ,EA4DI;AAAG,IAAA,SAAS,EAAC,wBAAb;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,khEA5DJ,EAkEI;AAAG,IAAA,SAAS,EAAC,wBAAb;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,+mCAlEJ,EAuEI;AAAG,IAAA,SAAS,EAAC,wBAAb;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACa;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,gBADb,0ZAvEJ,EA0EI;AAAG,IAAA,SAAS,EAAC,wBAAb;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACa;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,eADb,8YA1EJ,EA6EI;AAAG,IAAA,SAAS,EAAC,wBAAb;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACa;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,cADb,6SA7EJ,EAgFI;AAAG,IAAA,SAAS,EAAC,mBAAb;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,0YACoF;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,0BADpF,aAEI;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,yBAFJ,8xCAhFJ,CADJ;AAyFH,CA1FD;;AA4FA,eAAeC,SAAf","sourcesContent":["import React from 'react';\nimport '../../css/desktop.css';\nimport FirstPicture from './first-picture.jpg';\nimport './first-picture.css';\n\nconst Direction = () => {\n    return (\n        <div className=\"direction\">\n            <h2>Направления дальнейшего совершенствования Программы</h2>\n            <p className=\"mt-3 mb-0 text-justify\">\n                В идеале, Программа должна концентрировать не только лучшие практики, применяемые в Казани, а лучшие\n                практики, применяемые в России и на Западе. Для этого перед конструированием Программы полезно изучить\n                опыт работы Преподавателей со Студентами на кафедрах ведущих ВУЗ-ов России, в частности, МГУ и НИУ ВШЭ.\n            </p>\n            <p className=\"mt-3 mb-0 text-justify\">\n                Особое внимание стоит уделить открытой базе выпускных квалификационных работ студентов НИУ ВШЭ [3],\n                поскольку это одна из единственных баз, которая:\n            </p>\n            <p className=\"mt-3 mb-0 text-justify\">\n                1) принадлежит университету, по словам ряда преподавателей ИСФН КФУ, входящего в тройку лучших в стране\n                по качеству образования социологов;\n            </p>\n            <p className=\"mt-3 mb-0 text-justify\">\n                2) лишь единичные университеты публикуют в открытом доступе работы своих студентов, что связано с рядом\n                причин, таких как, низкое качество работ большинства студентов (что будет количественно подтверждено\n                далее. – Р.М.) и низкая репрезентативность информации в данных работах, засоряющих научное поле\n                малоприменимой информацией.\n            </p>\n            <p className=\"mt-3 mb-0 text-justify\">\n                Перед подготовкой данной работы была изучена структура вышеупомянутой базы НИУ ВШЭ посредством\n                описательных статистик. В ходе статистического одномерного анализа на выборочной совокупности в 20744\n                выпускных квалификационных работ (ВКР) было обнаружено следующее:\n            </p>\n            <p className=\"mt-3 mb-0 text-justify\">\n                – 72,37% Работ имеют оценку преподавательской комиссией НИУ ВШЭ менее 7 баллов, их полные версии не\n                опубликованы на сайте;\n            </p>\n            <p className=\"mt-3 mb-0 text-justify\">\n                – 22,99% Работ имеют оценку от 7 до 9 баллов, их полные версии опубликованы на сайте;\n            </p>\n            <p className=\"mt-3 mb-0 text-justify\">\n                – лишь 4,64% Работ имеют высшую оценку в 10 баллов (именно поэтому с них стоит брать пример при\n                конструировании Программы, опираясь на них как на лучшие практики и адаптируя некоторые из них к\n                требованиям написания работ в ИСФН КФУ. – Р.М.).\n            </p>\n            <p className=\"mt-3 mb-0 text-justify\">\n                Приведем характеристики выборочной совокупности, детализирующие устройство структуры проанализированной\n                базы (см. рис. 1):\n            </p>\n            <p className=\"mt-3 mb-0 text-justify\">– кампус / факультет: НИУ ВШЭ - Москва (без МИЭМ);</p>\n            <p className=\"mt-3 mb-0 text-justify\">– годы защиты: 2013-2018;</p>\n            <p className=\"mt-3 mb-0 text-justify\">– уровень образования: бакалавриат (42,93%) и магистратура (42,93%);</p>\n            <p className=\"mt-3 mb-0 text-justify\">– оценка: все баллы (от 1 до 10);</p>\n            <p className=\"mt-3 mb-0 text-justify\">– программа обучения: все, кроме программ МИЭМ;</p>\n            <p className=\"mt-3 mb-0 text-justify\">– язык ВКР: русский (87,11%) и английский (12,89%).</p>\n            <p className=\"mt-3 text-justify\">\n                Стоит отметить, что вышеописанная Программа не будет определять полную, конечную Оценку за Работу. Она\n                будет определять лишь ее часть, например, будет оценивать долю в 50% от проделанной Студентом Работы.\n                Причина этого состоит в том, что часть оценки преподаватели формируют по неформальным характеристикам\n                студента (соблюдении сроков и этапов выполнения работы, стилю общения с научным руководителем и т.д.).\n            </p>\n            <p className=\"table__number font-italic text-right mt-0 mb-0\">Рисунок 1</p><br/>\n            <p className=\"table__title font-weight-bold text-center mt-0\">\n                Процентное соотношение выпускных квалификационных работ студентов НИУ ВШЭ по степени их качества на\n                основе выборочной совокупности в 20744 работ (%). Источник: открытая база ВКР НИУ ВШЭ\n            </p>\n            <img src={FirstPicture} alt=\"Рисунок 1\" className=\"first-picture shadow\" />\n            <p className=\"mt-3 mb-0 text-justify\">\n                Поскольку для проверки качества Работы Студенту будет необходимо ответить более, чем на 30 вопросов, то\n                в Программе будет создана возможность регистрации и авторизации, что позволит сохранять предыдущие\n                ответы Студентов и восстанавливать их с небольшими правками спустя время, тем самым минимизируя рутинные\n                действия и экономя время Студентов, которое они смогут направить на улучшение Работы.\n            </p>\n            <p className=\"mt-3 mb-0 text-justify\">\n                Программу возможно адаптировать под потребности др. кафедр и ВУЗ-ов, создав систему поддоменов, где\n                страница (вопросы анкеты) каждого поддомена будет хранить  специфические требования конкретной\n                кафедры/ВУЗ-а. Пример:\n            </p>\n            <p className=\"mt-3 mb-0 text-justify\">\n                – http://<b>kfu-isfn</b>.u-ch.ru («Казанский Федеральный Университет», институт социально-философских наук);\n            </p>\n            <p className=\"mt-3 mb-0 text-justify\">\n                – http://<b>kfu-ipo</b>.u-ch.ru («Казанский Федеральный Университет», институт психологии и образования);\n            </p>\n            <p className=\"mt-3 mb-0 text-justify\">\n                – http://<b>hse-cs</b>.u-ch.ru («Высшая школа экономики», факультет компьютерных наук).\n            </p>\n            <p className=\"mt-3 text-justify\">\n                Планируемое название сайта программы – u-ch.ru (лаконичное сочетание англ. слов <i>«university»</i> и\n                <i>«checklist»</i>). На момент написания данной работы этот домен свободен для покупки и создания в\n                рамках него описанной программы. Второй планируемый вариант названия домена – univer-checklist.ru (хотя\n                данное название менее лаконично, скорее всего, его будет легче запомнить студентам).\n            </p>\n        </div>\n    )\n};\n\nexport default Direction;"]},"metadata":{},"sourceType":"module"}