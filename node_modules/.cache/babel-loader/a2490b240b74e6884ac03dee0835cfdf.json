1\\u0430\\u043B\\u043B\\u043E\\u0432\\xA0\\u2013 \\u201C\\u0445\\u043E\\u0440\\u043E\\u0448\\u043E\\u201D (\\u0445\\u043E\\u0440.);\", React.createElement(\"br\", {\n    __source: {\n      fileName: _jsxFileName,\n      lineNumber: 36\n    },\n    __self: this\n  }), \"\\u2013 55 \\u221270 \\u0431\\u0430\\u043B\\u043B\\u043E\\u0432\\xA0\\u2013 \\u201C\\u0443\\u0434\\u043E\\u0432\\u043B\\u0435\\u0442\\u0432\\u043E\\u0440\\u0438\\u0442\\u0435\\u043B\\u044C\\u043D\\u043E\\u201D (\\u0443\\u0434\\u043E\\u0432.);\", React.createElement(\"br\", {\n    __source: {\n      fileName: _jsxFileName,\n      lineNumber: 37\n    },\n    __self: this\n  }), \"\\u2013 54\\xA0\\u0431\\u0430\\u043B\\u043B\\u0430 \\u0438\\xA0\\u043C\\u0435\\u043D\\u0435\\u0435\\xA0\\u2013 \\u201C\\u043D\\u0435\\u0443\\u0434\\u043E\\u0432\\u043B\\u0435\\u0442\\u0432\\u043E\\u0440\\u0438\\u0442\\u0435\\u043B\\u044C\\u043D\\u043E\\u201D (\\u043D\\u0435\\u0443\\u0434.)\\xBB [4].\"));\n};\n\nexport default Concepts;","map":{"version":3,"sources":["/Users/r.v.mosolov/Desktop/webstorm-projects/projects/big-projects/university-checklist.ru/src/pages/about-service/about-service-full/site-content/blocks/concepts.js"],"names":["React","Concepts"],"mappings":";AAAA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAO,oBAAP;;AAEA,MAAMC,QAAQ,GAAG,MAAM;AACnB,SACI;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACI;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,qTADJ,EAEI;AAAG,IAAA,SAAS,EAAC,wBAAb;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACI;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KAAG;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,8DAAH,CADJ,i8DAFJ,EASI;AAAG,IAAA,SAAS,EAAC,wBAAb;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACI;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KAAG;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,4CAAH,CADJ,wuCATJ,EAcI;AAAG,IAAA,SAAS,EAAC,wBAAb;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACI;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KAAG;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,sFAAH,CADJ,ikDAdJ,EAoBI;AAAG,IAAA,SAAS,EAAC,wBAAb;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACI;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KAAG;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,kDAAH,CADJ,iWApBJ,EAuBI;AAAG,IAAA,SAAS,EAAC,mBAAb;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACI;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KAAG;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,4CAAH,CADJ,+sDAKI;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,IALJ,4LAMI;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,IANJ,yIAOI;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,IAPJ,uNAQI;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,IARJ,uQAvBJ,CADJ;AAoCH,CArCD;;AAuCA,eAAeA,QAAf","sourcesContent":["import React from 'react';\nimport '../css/desktop.css';\n\nconst Concepts = () => {\n    return (\n        <div>\n            <h2>Уточнения понятий, действующих в рамках настоящей работы</h2>\n            <p className=\"mt-3 mb-0 text-justify\">\n                <b><i>Программа</i></b> – это информационная система, функциональная совокупность которой\n                направлена на цели, ради достижения которых она была создана и которые будут описаны далее. Под\n                этим понятием, с технической точки зрения, будет подразумеваться сайт, размещенный в Интернете,\n                пользоваться которым будет возможно с любого наиболее популярного устройства (смартфона,\n                компьютера, ноутбука, планшета).\n            </p>\n            <p className=\"mt-3 mb-0 text-justify\">\n                <b><i>Работа</i></b> – это объект, на проверку и улучшение качества которого направлено внимание\n                сотрудников ИСФН КФУ. Его основным выражением являются курсовая или дипломная (ВКР) работы.\n                Аспирантура, в рамках данной информационной системы, не рассматривается.\n            </p>\n            <p className=\"mt-3 mb-0 text-justify\">\n                <b><i>Преподаватель</i></b> – это каждый участник, в той или иной степени участвующий в\n                подготовке курсовой или дипломной работы (научные руководители, члены комиссии, любые др.\n                преподаватели, часть деятельности которых направлена на регулярное напоминание студентам о\n                необходимости своевременной сдачи работы и коррекцию их ошибок).\n            </p>\n            <p className=\"mt-3 mb-0 text-justify\">\n                <b><i>Студент</i></b> – это участник всей совокупности студентов, обучающихся в ИСФН КФУ.\n            </p>\n            <p className=\"mt-3 text-justify\">\n                <b><i>Оценка</i></b> – это количественный индикатор, измеряемый в баллах, позволяющий оценить\n                качество проделанной студентом работы, где каждый балл принадлежит к определенному диапазону,\n                выражающему характерный для школьной системы РФ тип оценки («тройку», «четверку», «пятерку»).\n                Согласно «Регламенту о балльно-рейтинговой системе знаний…» КФУ от 02.08.2012:\n                <br/>«– 86 баллов и более – “отлично” (отл.);\n                <br/>– 71-85 баллов – “хорошо” (хор.);\n                <br/>– 55 −70 баллов – “удовлетворительно” (удов.);\n                <br/>– 54 балла и менее – “неудовлетворительно” (неуд.)» [4].\n            </p>\n        </div>\n    )\n};\n\nexport default Concepts;"]},"metadata":{},"sourceType":"module"}